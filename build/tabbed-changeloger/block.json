{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "cha/tabbed-changeloger",
  "version": "0.1.0",
  "title": "Tabbed Changeloger",
  "category": "widgets",
  "icon": "smiley",
  "description": "A tabbed interface for displaying multiple changelogs.",
  "example": {},
  "attributes": {
    "activeTab": {
      "type": "number",
      "default": 0
    },
    "tabs": {
      "type": "array",
      "default": [
        {
          "title": "Tab 1",
          "id": "tab-1"
        }
      ]
    },
    "tabAlignment": {
      "type": "string",
      "default": "left"
    }
  },
  "supports": {
    "html": false
  },
  "providesContext": {
    "cha/activeTab": "activeTab",
    "cha/tabs": "tabs"
  },
  "textdomain": "tabbed-changeloger",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "tabbed-changeloger",
  "viewScript": "tabbed-changeloger-frontend"
}